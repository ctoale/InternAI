{
  "name": "itinerai",
  "version": "1.0.0",
  "description": "AI-powered travel planning application",
  "main": "src/server/index.ts",
  "scripts": {
    "start": "node dist/server/index.js",
    "dev": "nodemon src/server/index.ts",
    "build": "tsc",
    "build:server": "tsc -p tsconfig.server.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "test": "jest",
    "start:node": "nodemon src/server/index.ts",
    "start:backend": "concurrently \"npm run dev\" \"py src/python/server.py\"",
    "start:all": "concurrently \"npm run dev\" \"py src/python/server.py\" \"cd client && npm start\""
  },
  "dependencies": {
    "@prisma/client": "^5.10.0",
    "@types/axios": "^0.9.36",
    "@types/mongoose": "^5.11.96",
    "axios": "^1.8.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.13.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.16",
    "concurrently": "^9.1.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prisma": "^5.10.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  }
}
